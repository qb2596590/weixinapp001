page {
    background-color: rgba(231, 229, 229, 0.26);
    --notice-bar-height: 40rpx;
}

.main {
    width: 100%;
    height: 30vh;
}

.downLoadArea {

    width: 100%;
    /* height: 20vh; */
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-bottom: 100rpx;
}

.dl_left {
    background-color: white;
    border: 1px solid rgba(167, 164, 164, 0.438);
    border-radius: 50rpx;
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: center;
    line-height: 80rpx;
}

.hover_click {
    opacity: 0.9;

    box-shadow: 0px 0px 8px rgba(0, 0, 0, .1) inset;
}
.size{
    width: 60rpx;
    height: 60rpx;
}
.icontxt{
    margin-top: 5rpx;
    font-size: 26rpx;
}